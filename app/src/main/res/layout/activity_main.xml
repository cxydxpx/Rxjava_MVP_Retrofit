        android:text="RxJava" />


    <Button
        android:onClick="rxJavaDemo2"
        android:id="@+id/btn_RxJava2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="RxJava2" />
</LinearLayout>
